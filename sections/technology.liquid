<!-- start of technology-section -->
<section class="technology-section section-padding px-60">
  <div class="container-fluid">
    <div class="technology-title">
      <h3>{{ section.settings.section_title }}</h3>
    </div>
    <div class="row">
      <div class="col-lg-12 col-12">
        <div class="technology-item">
          <div class="part-wrap">
            <div class="part-left">
              <div class="part-img wow fadeInLeftSlow" data-wow-duration="2000ms">
                <img
                  src="{{section.settings.part_three_image | image_url: width:1000 }}"
                  alt=""
                  width=""
                  height="">
                <div class="info">
                  <div class="thumb">
                    <span>3</span>
                  </div>
                  <div class="info-text">
                    <h3>{{ section.settings.part_three_title }}</h3>
                    <p>{{ section.settings.part_three_description }}</p>
                  </div>
                </div>
              </div>
              <div class="part-img2 wow fadeInLeftSlow" data-wow-duration="1500ms">
                <img
                  src="{{section.settings.part_three_image_sub |  image_url: width:1000 }}"
                  width=""
                  height=""
                  alt="">
              </div>
            </div>
            <div class="part-right">
              <div class="part-img wow fadeInRightSlow" data-wow-duration="1500ms">
                <img
                  src="{{section.settings.part_two_image |  image_url: width: 1000 }}"
                  width=""
                  height=""
                  alt="">
              </div>
              <div class="part-img2 wow fadeInRightSlow" data-wow-duration="2000ms">
                <img
                  src="{{section.settings.part_two_image_sub |  image_url: width: 1000 }}"
                  width=""
                  height=""
                  alt="">
                <div class="info s2">
                  <div class="thumb">
                    <span>2</span>
                  </div>
                  <div class="info-text">
                    <h3>{{ section.settings.part_two_title }}</h3>
                    <p>{{ section.settings.part_two_description }}</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="part-bottom">
            <div class="part-img wow fadeInUp" data-wow-duration="1500ms">
              <img
                src="{{ section.settings.part_five_image |  image_url: width: 1000 }}"
                width=""
                height=""
                alt="">
              <div class="info">
                <div class="thumb">
                  <span>5</span>
                </div>
                <div class="info-text">
                  <h3>{{ section.settings.part_five_title }}</h3>
                  <p>{{ section.settings.part_five_description }}</p>
                </div>
              </div>
              <div class="info s2">
                <div class="thumb">
                  <span>4</span>
                </div>
                <div class="info-text">
                  <h3>{{ section.settings.part_four_title }}</h3>
                  <p>{{ section.settings.part_four_description }}</p>
                </div>
              </div>
            </div>
            <div class="part-img2 wow fadeInUp" data-wow-duration="1700ms">
              <img
                src="{{ section.settings.part_one_image |  image_url: width: 1000 }}"
                alt=""
                width=""
                height="">
              <div class="info s2">
                <div class="thumb">
                  <span>1</span>
                </div>
                <div class="info-text">
                  <h3>{{ section.settings.part_one_title }}</h3>
                  <p>{{ section.settings.part_one_description }}</p>
                </div>
              </div>
            </div>
          </div>
          <div class="part-img3 wow fadeInUp" data-wow-duration="1900ms">
            <div class="part-img-l">
              <img
                src="{{ section.settings.part_one_image_sub |  image_url: width: 1000 }}"
                width=""
                height=""
                alt="">
            </div>
            <div class="part-img-l">
              <img
                src="{{ section.settings.part_one_image_sub_2 |  image_url: width: 1000 }}"
                width=""
                height=""
                alt="">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>


